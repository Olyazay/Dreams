<Window x:Class="DreamHouse.Pages.Category"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DreamHouse.Pages"
        xmlns:viewmodal="clr-namespace:DreamHouse.ViewModels"
        mc:Ignorable="d"
        Title="Category" Height="3840" Width="2160">
    <Window.DataContext>
        <viewmodal:MainWindowViewModel/>
    </Window.DataContext>
    <Grid>
        <TextBlock>NameCategory</TextBlock>
        <ListBox ItemsSource="{Binding ChoosenCategory}">
            <ListBox.ItemTemplate>
                <DataTemplate>
                    <TextBlock ></TextBlock>
                </DataTemplate>
            </ListBox.ItemTemplate>
        </ListBox>
    </Grid>
</Window>
